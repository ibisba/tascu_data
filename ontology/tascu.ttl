###  http://empusa.org/0.1/defaultEnumerationValue
empusa:defaultEnumerationValue rdf:type owl:AnnotationProperty ;
                               rdfs:comment "Used to denote a certain entry within an enumeration to be default value. This is commonly 'anyOtherXXX'" ;
                               rdfs:subPropertyOf rdfs:comment ;
                               rdfs:range xsd:boolean .


###  http://empusa.org/0.1/propertyDefinitions
empusa:propertyDefinitions rdf:type owl:AnnotationProperty ;
                           rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/TODO
<http://gbol.life/0.1/TODO> rdf:type owl:AnnotationProperty ;
                            rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/ddbjLabel
<http://gbol.life/0.1/ddbjLabel> rdf:type owl:AnnotationProperty ;
                                 rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/framing
<http://gbol.life/0.1/framing> rdf:type owl:AnnotationProperty ;
                               rdfs:comment """If class is the root denote it with
ROOT
Any childs for the  framing  can be denoted with
prefix:prop1
prefix:prop2""" ;
                               skos:definition "This annotation property defines the JSON-LD framing" ;
                               rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/regex
<http://gbol.life/0.1/regex> rdf:type owl:AnnotationProperty ;
                             rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/subDomain
<http://gbol.life/0.1/subDomain> rdf:type owl:AnnotationProperty ;
                                 rdfs:subPropertyOf rdfs:comment .


###  http://gbol.life/0.1/valueFormat
<http://gbol.life/0.1/valueFormat> rdf:type owl:AnnotationProperty ;
                                   rdfs:comment "format of qualifier as specified in ddbj format" ;
                                   rdfs:subPropertyOf rdfs:comment .


###  http://ssb.wur.nl/OwlShexCodeGen#defaultEnumerationValue
<http://ssb.wur.nl/OwlShexCodeGen#defaultEnumerationValue> rdf:type owl:AnnotationProperty ;
                                                           rdfs:comment "Used to denote a certain entry within an enumeration to be default value. This is commonly 'anyOtherXXX'" ;
                                                           rdfs:subPropertyOf rdfs:comment ;
                                                           rdfs:range xsd:boolean .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#deprecated
owl:deprecated rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#changeNote
skos:changeNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#editorialNote
skos:editorialNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#historyNote
skos:historyNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://gbol.life/0.1/number
<http://gbol.life/0.1/number> rdf:type owl:ObjectProperty ;
                              <http://gbol.life/0.1/ddbjLabel> "number" ;
                              <http://gbol.life/0.1/regex> "^\\s*(\\d+.*|[XVILC]+[[:alnum:]]*)\\s*$" ;
                              <http://gbol.life/0.1/valueFormat> "unquoted text (single token)" ;
                              rdfs:comment "text limited to integers, letters or combination of integers and/orletters represented as an unquoted single token (e.g. 5a, XIIb);any additional terms should be included in /standard_name.Example:  /number=2A/standard_name=\"long\"" ;
                              skos:definition "a number to indicate the order of genetic elements (e.g.,exons or introns) in the 5' to 3' direction" ;
                              skos:editorialNote "Removed, denote this with position before or after in the location object" ,
                                                 "Replaced with the exon list" ;
                              skos:example "/number=4/number=6B" .


###  http://purl.org/NET/c4dm/event.owl#place
event:place rdf:type owl:ObjectProperty ;
            skos:scopeNote "Used to relate an event such as a conference to the geographical place where it happens, for example Paris."@en .


###  http://purl.org/NET/c4dm/event.owl#produced_in
event:produced_in rdf:type owl:ObjectProperty .


###  http://purl.org/NET/c4dm/event.owl#product
event:product rdf:type owl:ObjectProperty ;
              skos:scopeNote "Used to link an event such as a conference to an outcome (a product) of that event, for example, an article, a proceeding, etc."@en .


###  http://purl.org/NET/c4dm/event.owl#sub_event
event:sub_event rdf:type owl:ObjectProperty ;
                skos:scopeNote "Used to link big events with smaller events such as workshops that happen in the context of a conference."@en .


###  http://purl.org/NET/c4dm/event.owl#time
event:time rdf:type owl:ObjectProperty ;
           skos:scopeNote "Used to describe the timing of an event. For example, when a conference starts and stops."@en .


###  http://purl.org/dc/terms/contributor
dc:contributor rdf:type owl:ObjectProperty ;
               skos:scopeNote "Used to link a bibliographic item to one of its contributor: can be an author, an editor, a publisher, etc."@en .


###  http://purl.org/dc/terms/format
dc:format rdf:type owl:ObjectProperty ;
          skos:example """<dc:format>
   <dc:MediaTypeOrExtent>
     <rdf:value>text/html</rdf:value>
     <rdfs:label>HTML</rdfs:label>
   </dc:MediaTypeOrExtent>
 </dc:format>"""@en ;
          skos:scopeNote "Used to describe the format of a bibliographic resource."@en .


###  http://purl.org/dc/terms/hasPart
dc:hasPart rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isPartOf
dc:isPartOf rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isReferencedBy
dc:isReferencedBy rdf:type owl:ObjectProperty ;
                  skos:scopeNote "Used to relate a reference citation to a bibliographic resource."@en .


###  http://purl.org/dc/terms/isVersionOf
dc:isVersionOf rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/language
dc:language rdf:type owl:ObjectProperty ;
            skos:scopeNote "Used to link a bibliographic resource to the language used to express it."@en .


###  http://purl.org/dc/terms/publisher
dc:publisher rdf:type owl:ObjectProperty ;
             skos:scopeNote "Used to link a bibliographic item to its publisher."@en .


###  http://purl.org/dc/terms/references
dc:references rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/relation
dc:relation rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/rights
dc:rights rdf:type owl:ObjectProperty ;
          skos:scopeNote "Used to describe rights related to a bibliographic resource."@en .


###  http://purl.org/dc/terms/subject
dc:subject rdf:type owl:ObjectProperty ;
           skos:scopeNote "Used to describe the subject of a bibliographic resource."@en .


###  http://purl.org/dc/terms/title
dc:title rdf:type owl:ObjectProperty .


###  http://purl.org/ontology/bibo/affirmedBy
bibo:affirmedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bibo:subsequentLegalDecision ;
                rdfs:comment "A legal decision that affirms a ruling."@en ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/annotates
bibo:annotates rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dc:relation ;
               rdfs:comment "Critical or explanatory note for a Document."@en ;
               rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
               rdfs:label "annotates"@en .


###  http://purl.org/ontology/bibo/authorList
bibo:authorList rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bibo:contributorList ;
                rdfs:comment "An ordered list of authors. Normally, this list is seen as a priority list that order authors by importance."@en ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                rdfs:label "list of authors"@en .


###  http://purl.org/ontology/bibo/cites
bibo:cites rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf dc:references ;
           rdfs:comment """Relates a document to another document that is cited
by the first document as reference, comment, review, quotation or for
another purpose."""@en ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "cites"@en .


###  http://purl.org/ontology/bibo/contributorList
bibo:contributorList rdf:type owl:ObjectProperty ;
                     rdfs:comment "An ordered list of contributors. Normally, this list is seen as a priority list that order contributors by importance."@en ;
                     rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                     rdfs:label "list of contributors"@en .


###  http://purl.org/ontology/bibo/degree
bibo:degree rdf:type owl:ObjectProperty ;
            rdfs:comment "The thesis degree."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "degree"@en ;
            skos:editorialNote "We are not defining, using an enumeration, the range of the bibo:degree to the defined list of bibo:ThesisDegree. We won't do it because we want people to be able to define new degress if needed by some special usecases. Creating such an enumeration would restrict this to happen."@en .


###  http://purl.org/ontology/bibo/director
bibo:director rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dc:contributor ;
              rdfs:comment "A Film director."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "director" .


###  http://purl.org/ontology/bibo/distributor
bibo:distributor rdf:type owl:ObjectProperty ;
                 rdfs:comment "Distributor of a document or a collection of documents."@en ;
                 rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                 rdfs:label "distributor"@en .


###  http://purl.org/ontology/bibo/editor
bibo:editor rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf dc:contributor ;
            rdfs:comment "A person having managerial and sometimes policy-making responsibility for the editorial part of a publishing firm or of a newspaper, magazine, or other publication."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "editor" .


###  http://purl.org/ontology/bibo/editorList
bibo:editorList rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bibo:contributorList ;
                rdfs:comment "An ordered list of editors. Normally, this list is seen as a priority list that order editors by importance."@en ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                rdfs:label "list of editors"@en .


###  http://purl.org/ontology/bibo/interviewee
bibo:interviewee rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dc:contributor ;
                 rdfs:comment "An agent that is interviewed by another agent."@en ;
                 rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                 rdfs:label "interviewee" .


###  http://purl.org/ontology/bibo/interviewer
bibo:interviewer rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dc:contributor ;
                 rdfs:comment "An agent that interview another agent."@en ;
                 rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                 rdfs:label "interviewer" .


###  http://purl.org/ontology/bibo/issuer
bibo:issuer rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf dc:publisher ;
            rdfs:comment "An entity responsible for issuing often informally published documents such as press releases, reports, etc." ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "issuer" .


###  http://purl.org/ontology/bibo/organizer
bibo:organizer rdf:type owl:ObjectProperty ;
               rdfs:comment "The organizer of an event; includes conference organizers, but also government agencies or other bodies that are responsible for conducting hearings."@en ;
               rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
               rdfs:label "organizer"@en .


###  http://purl.org/ontology/bibo/owner
bibo:owner rdf:type owl:ObjectProperty ;
           rdfs:comment "Owner of a document or a collection of documents."@en ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "owner"@en .


###  http://purl.org/ontology/bibo/performer
bibo:performer rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dc:contributor ;
               rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
               rdfs:label "performer" .


###  http://purl.org/ontology/bibo/presentedAt
bibo:presentedAt rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf event:produced_in ;
                 owl:inverseOf bibo:presents ;
                 rdfs:comment "Relates a document to an event; for example, a paper to a conference."@en ;
                 rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                 rdfs:label "presented at"@en .


###  http://purl.org/ontology/bibo/presents
bibo:presents rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf event:product ;
              rdfs:comment "Relates an event to associated documents; for example, conference to a paper."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "presents"@en .


###  http://purl.org/ontology/bibo/producer
bibo:producer rdf:type owl:ObjectProperty ;
              rdfs:comment "Producer of a document or a collection of documents."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "producer"@en .


###  http://purl.org/ontology/bibo/recipient
bibo:recipient rdf:type owl:ObjectProperty ;
               rdfs:comment "An agent that receives a communication document."@en ;
               rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
               rdfs:label "recipient" .


###  http://purl.org/ontology/bibo/reproducedIn
bibo:reproducedIn rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dc:isPartOf ;
                  rdfs:comment "The resource in which another resource is reproduced."@en ;
                  rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/reversedBy
bibo:reversedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bibo:subsequentLegalDecision ;
                rdfs:comment "A legal decision that reverses a ruling."@en ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/reviewOf
bibo:reviewOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dc:relation ;
              rdfs:comment "Relates a review document to a reviewed thing (resource, item, etc.)."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "review of"@en .


###  http://purl.org/ontology/bibo/status
bibo:status rdf:type owl:ObjectProperty ;
            rdfs:comment "The publication status of (typically academic) content."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "status"@en ;
            skos:editorialNote "We are not defining, using an enumeration, the range of the bibo:status to the defined list of bibo:DocumentStatus. We won't do it because we want people to be able to define new status if needed by some special usecases. Creating such an enumeration would restrict this to happen."@en .


###  http://purl.org/ontology/bibo/subsequentLegalDecision
bibo:subsequentLegalDecision rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf dc:isReferencedBy ;
                             rdfs:comment "A legal decision on appeal that takes action on a case (affirming it, reversing it, etc.)."@en ;
                             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/transcriptOf
bibo:transcriptOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dc:relation ;
                  rdfs:comment "Relates a document to some transcribed original."@en ;
                  rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                  rdfs:label "transcript of"@en .


###  http://purl.org/ontology/bibo/translationOf
bibo:translationOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dc:isVersionOf ;
                   rdfs:comment "Relates a translated document to the original document."@en ;
                   rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                   rdfs:label "translation of"@en .


###  http://purl.org/ontology/bibo/translator
bibo:translator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dc:contributor ;
                rdfs:comment "A person who translates written document from one language to another."@en ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                rdfs:label "translator" .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:ObjectProperty ;
          skos:scopeNote """Used to describe the content of a bibo:Document and othr bibliographic resouces.

We suggest to use this property instead of the deprecated \"bibo:content\" one."""@en .


###  http://xmlns.com/foaf/0.1/based_near
foaf:based_near rdf:type owl:ObjectProperty ;
                skos:scopeNote "Used to link an agent, related to bibliographic things, to a place where it is based near: can be a city, a monument, a building, etc."@en .


###  http://xmlns.com/foaf/0.1/depiction
foaf:depiction rdf:type owl:ObjectProperty ;
               skos:scopeNote "Used to link an agent with an image that depict it."@en .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:ObjectProperty ;
              skos:scopeNote "Used to link an agent to its homepage (which is a web page accessible using a URL)."@en .


#################################################################
#    Data properties
#################################################################

###  http://prismstandard.org/namespaces/1.2/basic/doi
prism:doi rdf:type owl:DatatypeProperty ;
          owl:equivalentProperty bibo:doi .


###  http://prismstandard.org/namespaces/1.2/basic/eIssn
prism:eIssn rdf:type owl:DatatypeProperty ;
            owl:equivalentProperty bibo:eissn .


###  http://prismstandard.org/namespaces/1.2/basic/edition
prism:edition rdf:type owl:DatatypeProperty ;
              owl:equivalentProperty bibo:edition .


###  http://prismstandard.org/namespaces/1.2/basic/endingPage
prism:endingPage rdf:type owl:DatatypeProperty ;
                 owl:equivalentProperty bibo:pageEnd .


###  http://prismstandard.org/namespaces/1.2/basic/isbn
prism:isbn rdf:type owl:DatatypeProperty ;
           owl:equivalentProperty bibo:isbn .


###  http://prismstandard.org/namespaces/1.2/basic/issn
prism:issn rdf:type owl:DatatypeProperty ;
           owl:equivalentProperty bibo:issn .


###  http://prismstandard.org/namespaces/1.2/basic/issue
prism:issue rdf:type owl:DatatypeProperty ;
            owl:equivalentProperty bibo:issue .


###  http://prismstandard.org/namespaces/1.2/basic/number
prism:number rdf:type owl:DatatypeProperty ;
             owl:equivalentProperty bibo:locator .


###  http://prismstandard.org/namespaces/1.2/basic/startingPage
prism:startingPage rdf:type owl:DatatypeProperty ;
                   owl:equivalentProperty bibo:pageStart .


###  http://prismstandard.org/namespaces/1.2/basic/volume
prism:volume rdf:type owl:DatatypeProperty ;
             owl:equivalentProperty bibo:volume .


###  http://purl.org/dc/terms/created
dc:created rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf dc:date ;
           skos:scopeNote "Used to describe the creation date of a bibliographic item"@en .


###  http://purl.org/dc/terms/date
dc:date rdf:type owl:DatatypeProperty ;
        skos:scopeNote "Use to link a bibliographic item to the date of an event. Check dc:created and other for proper specializations for this property"@en .


###  http://purl.org/dc/terms/description
dc:description rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/terms/issued
dc:issued rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf dc:date ;
          skos:scopeNote "Used to describe the issue date of a bibliographic resource"@en .


###  http://purl.org/ontology/bibo/abstract
bibo:abstract rdf:type owl:DatatypeProperty ;
              rdfs:comment "A summary of the resource." ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "abstract" .


###  http://purl.org/ontology/bibo/argued
bibo:argued rdf:type owl:DatatypeProperty ;
            rdfs:comment "The date on which a legal case is argued before a court. Date is of format xsd:date"@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "date argued"@en .


###  http://purl.org/ontology/bibo/asin
bibo:asin rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf bibo:identifier .


###  http://purl.org/ontology/bibo/chapter
bibo:chapter rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf bibo:locator ;
             rdfs:comment "An chapter number"@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "chapter"@en .


###  http://purl.org/ontology/bibo/coden
bibo:coden rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf bibo:identifier .


###  http://purl.org/ontology/bibo/content
bibo:content rdf:type owl:DatatypeProperty ;
             rdfs:comment "This property is for a plain-text rendering of the content of a Document. While the plain-text content of an entire document could be described by this property."@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "content"@en ;
             owl:deprecated "true"^^xsd:boolean ;
             skos:historyNote "bibo:content has been deprecated; we recommend to use \"rdf:value\" for this purpose. Here is the rational behind this choice: http://www.w3.org/TR/2004/REC-rdf-primer-20040210/#rdfvalue"@en .


###  http://purl.org/ontology/bibo/doi
bibo:doi rdf:type owl:DatatypeProperty ;
